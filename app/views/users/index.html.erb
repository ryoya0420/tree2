<div>
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3 user-index-container">
        <h1>ユーザ一覧</h1>
        <table class="table">
          <tr>
            <th>氏名</th>
            <th>email</th>
          </tr>
          <% @users.each do |user| %>
          <tr>
            <th>
              <%= link_to user.name, user_path(user.id) %>
            </th>
            <th>
              <%= user.email %>
            </th>
          </tr>
          <% end %>
        </table>
      </div>
    </div>
  </div>
</div>